# Comput3.ai API Key - REQUIRED
C3_API_KEY=your_comput3_api_key_here

# Redis configuration
REDIS_HOST=redis
REDIS_PORT=6379

# Minio configuration
MINIO_ENDPOINT=your_minio_endpoint
MINIO_ACCESS_KEY=your_minio_access_key
MINIO_SECRET_KEY=your_minio_secret_key
MINIO_SECURE=false

# GPU management configuration
GPU_IDLE_TIMEOUT=300
RENDER_POLLING_INTERVAL=5

# Job timing configuration
DEFAULT_COMPLETE_IN=3600
DEFAULT_MAX_TIME=1200

# Worker delay configuration
GPU_WORKER_DELAY=1
NO_GPU_WORKER_DELAY=5
LOCK_RETRY_INTERVAL=0.5
QUEUE_CHECK_INTERVAL=0.5

# For local development, use:
# REDIS_HOST=localhost
# REDIS_PORT=6379